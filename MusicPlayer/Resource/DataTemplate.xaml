<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:uc="clr-namespace:MusicPlayer.UserControls"
                    xmlns:viewmodel="clr-namespace:MusicPlayer.ViewModel">
    <DataTemplate DataType="{x:Type viewmodel:HomeVM}">
        <uc:UCHome/>
    </DataTemplate>
    <DataTemplate DataType="{x:Type viewmodel:LibraryVM}">
        <uc:UCLibrary/>
    </DataTemplate>
    <DataTemplate DataType="{x:Type viewmodel:SearchVM}">
        <uc:UCSearch/>
    </DataTemplate>
    <DataTemplate DataType="{x:Type viewmodel:ProfileVM}">
        <uc:UCProfile/>
    </DataTemplate>

</ResourceDictionary>